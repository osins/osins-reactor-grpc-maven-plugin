    <modelVersion>4.0.0</modelVersion>

    <groupId>io.osins.grpc</groupId>
    <artifactId>osins-reactor-grpc-maven-plugin</artifactId>
    <version>1.76.0.0</version>
    <packaging>maven-plugin</packaging>

    <name>osins-reactor-grpc-maven-plugin</name>
    <description>自动生成 gRPC Stub 的 Spring Bean</description>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <project.reporting.outputEncoding>UTF-8</project.reporting.outputEncoding>
        <maven.compiler.source>21</maven.compiler.source>
        <maven.compiler.target>21</maven.compiler.target>
        <maven.resolver.version>1.9.23</maven.resolver.version>
        <maven.version>3.9.10</maven.version>
        <grpc.version>1.76.0</grpc.version>
        <protobuf.version>4.33.0</protobuf.version>
        <aether.version>1.1.0</aether.version>

        <surefire.redirectTestOutputToFile>true</surefire.redirectTestOutputToFile>
        <failsafe.redirectTestOutputToFile>${surefire.redirectTestOutputToFile}</failsafe.redirectTestOutputToFile>
        <checkstyle.violation.ignore>None</checkstyle.violation.ignore>
        <sisuVersion>0.9.0.M4</sisuVersion>
        <guiceVersion>5.1.0</guiceVersion>
        <slf4jVersion>2.0.17</slf4jVersion>
        <bouncycastleVersion>1.81</bouncycastleVersion>
        <jettyVersion>10.0.25</jettyVersion>
        <bnd.instructions.additions />
    </properties>

    <dependencies>

        <dependency>
            <groupId>io.osins.matrix</groupId>
            <artifactId>matrix-shared-grpc-base</artifactId>
            <version>0.0.1</version>
        </dependency>

