            <version>1.18.38</version>
        </dependency>

        <dependency>
            <groupId>fr.inria.gforge.spoon</groupId>
            <artifactId>spoon-core</artifactId>
            <version>11.2.1</version>
            <exclusions>
                <exclusion>
                    <artifactId>commons-codec</artifactId>
